<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test  name="FlightBooking 1 passenger">
    <parameter  name="noOfPassengers" value="1"/>  
    <parameter name="expectedPrice" value="584" />
    <classes>
      <class name="testcases.BookFlightTest"/>
    </classes>
  </test> 
  <test  name="FlightBooking 2 passenger">
    <parameter  name="noOfPassengers" value="2"/>  
    <parameter name="expectedPrice" value="1169" />
    <classes>
      <class name="testcases.BookFlightTest"/>
    </classes>
  </test> 
  <test  name="FlightBooking 3 passenger">
    <parameter  name="noOfPassengers" value="3"/>  
    <parameter name="expectedPrice" value="1753" />
    <classes>
      <class name="testcases.BookFlightTest"/>
    </classes>
  </test> 
  <test  name="FlightBooking 4 passenger">
    <parameter  name="noOfPassengers" value="4"/>  
    <parameter name="expectedPrice" value="2338" />
    <classes>
      <class name="testcases.BookFlightTest"/>
    </classes>
  </test> 
 
  
</suite>
